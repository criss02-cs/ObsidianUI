        <Grid x:Name="Header" VerticalOptions="Fill" HorizontalOptions="Fill"/>
        <Grid x:Name="Month" VerticalOptions="Fill" HorizontalOptions="Fill" RowSpacing="-1" ColumnSpacing="-1"/>
    </StackLayout>
</ContentView>
